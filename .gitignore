# IRH_Suite v9.2 .gitignore

# Output directory (generated at runtime)
io/output/*.irh
io/output/*.csv
io/output/*.pdf
io/output/*.json
io/output/*.png
io/output/checkpoint_*.irh

# Large output files (use Git LFS)
*.irh

# Keep directory structure
!io/output/.gitkeep
!io/input/.gitkeep

# Temporary files
*.tmp
*.bak
*~

# Mathematica-specific
*.mx
*.m~
*.wl~
*.nb
.Mathematica/
FrontEnd/

# macOS
.DS_Store

# Windows
Thumbs.db
desktop.ini

# IDE/Editor
.vscode/
.idea/
*.swp
*.swo

# Log files (keep example, ignore generated)
*.log
logs/

# Cache files
*.cache
__pycache__/
.pytest_cache/
.mypy_cache/

# Python
*.pyc
*.pyo
*.egg-info/
dist/
build/
*.egg
.eggs/
pip-wheel-metadata/

# Virtual environments
venv/
.venv/
env/
.env/

# Jupyter
.ipynb_checkpoints/
*.ipynb_checkpoints

# Coverage
.coverage
coverage.xml
htmlcov/

# Archives
archives/*.tar.gz
archives/*.zip

# Pre-registration (keep YAML, ignore generated)
pre_reg/*.hash

# Orchestrator outputs and runtime files
crash_report_for_llm.txt
outputs/
config.json
